% a = alpha(v)
% b = alpha(a)
% d = d(a)
syms d a x0 b

dv = a+d;
db = dv - sqrt(a^2+b^2);
x = (b^2+db^2-d^2)/(2*b);
y = sqrt(d^2-x^2);
f = d + a - sqrt((a+y)^2+x^2);
f = simplify(f)
g = simplify(diff(f, d))
g = subs(g, a, 1);
g = subs(g, b, 1/2)
%solve(g < 0, d = (1/2)*sqrt(2)/2..(1/2)*sqrt(2))
%solve(g < 0, 'd=(1/2)*sqrt(2)/2..(1/2)*sqrt(2)')
%solve(g<0)
%s = ['1 - (d + (2*d - (66447228468885*d^2)/140737488355328 + ' ...
%       '(66447228468885*(d - 5^(1/2)/2 + ' ...
%      '1)^2)/140737488355328 + ' ...
%       '66447228468885/562949953421312)/(2*(d^2 - ((d - ' ...
%       '5^(1/2)/2 + ' ...
%       '1)^2 - d^2 + 1/4)^2)^(1/2)))/(2*(d^2 - ((d - 5^(1/2)/2 + ' ...
%       '1)^2 - d^2 + 1/4)^2)^(1/2) + d^2 + 1)^(1/2)']
%solve(s)

